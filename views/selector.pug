extends layout
block content
  h1= title
  - var image_names=["cinder_novel.jpg","aldous_novel.jpg","verity_novel.jpg","potter_novel.jpg","bhagat_novel.jpg"]
  - for(let i=0;i<3;i++){
     - let random= Math.floor(Math.random()*5)
     img(src=`/images/${image_names[random]}` alt=`Image No ${random}` height="300px" width="300px")
  - }
